  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:debug": "vitest --inspect-brk --runInBand",
    "test:e2e": "vitest --config ./test/vitest-e2e.config.ts",
    "create-database": "npx ts-node scripts/create-database.ts",
    "prisma:truncate": "npm run build && node dist/scripts/prisma-truncate.js",
    "seed:roles": "npm run build && node dist/scripts/seed-roles.js",
    "prisma:truncate-seed:roles": "npm run build && node dist/scripts/prisma-truncate.js && node dist/scripts/seed-roles.js",
    "db:setup": "npm run create-database && prisma migrate deploy && prisma generate && npm run build && node dist/scripts/seed-roles.js",
    "seed": "npm run build && node dist/scripts/seed-roles.js && node dist/scripts/seed-owner.js",
    "seed:owner": "npm run build && node dist/scripts/seed-owner.js",
    "prisma:generate": "prisma generate",
    "prisma:setup": "prisma migrate deploy && prisma generate",
    "prisma:sync": "npx ts-node scripts/prisma-sync.ts",
    "prisma:reset": "npx prisma migrate reset && npm run prisma:generate",
    "prisma:studio": "npx prisma studio"
  },
